{
    "name": "@eventstore/docs",
    "private": true,
    "version": "0.0.65",
    "description": "Documentation for Event Store Design System",
    "license": "Apache-2.0",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/",
        "access": "restricted"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:EventStore/Design-System.git",
        "directory": "documentation"
    },
    "scripts": {
        "build": "run-s build:*",
        "build:clean": "rm -rf ./www",
        "build:preview": "stencil build --dev --config ./preview/stencil.config.ts",
        "build:docs": "stencil build --prerender",
        "dev": "stencil build --dev --watch --serve",
        "prod": "stencil build --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "icon": "icon --dir ./src/icons"
    },
    "dependencies": {
        "@eventstore/components": "^0.0.65",
        "@eventstore/editor": "^0.0.65",
        "@eventstore/fields": "^0.0.65",
        "@eventstore/layout": "^0.0.65",
        "@eventstore/router": "^0.0.65",
        "@eventstore/stores": "^0.0.65",
        "@eventstore/utils": "^0.0.65",
        "highlight.js": "11.2.0",
        "magic-string": "0.25.7",
        "markdown-it": "12.2.0",
        "markdown-it-anchor": "8.1.3",
        "prettier": "2.0.5",
        "rollup": "2.33.1"
    },
    "devDependencies": {
        "@eventstore/configs": "^0.0.65",
        "@eventstore/icon-manager": "^0.0.65",
        "@eventstore/postcss-palette-plugin": "^0.0.65",
        "@stencil/core": "^2.7.1",
        "@stencil/postcss": "2.0.0",
        "@types/jest": "25.2.1",
        "@types/markdown-it": "12.0.3",
        "eslint": "7.3.1",
        "jest": "26.6.3",
        "jest-cli": "24.9.0",
        "postcss-preset-env": "6.7.0",
        "puppeteer": "^10.2.0",
        "rollup-plugin-node-polyfills": "0.2.1",
        "rollup-plugin-require-context": "0.0.2",
        "rollup-plugin-string": "3.0.0",
        "serve": "12.0.0",
        "typedoc": "0.21.5",
        "typescript": "4.2.3",
        "workbox-build": "5.1.3"
    }
}
