{
    "name": "@eventstore/components",
    "version": "0.1.0-alpha.0",
    "description": "Core components for Event Store Design System",
    "license": "Apache-2.0",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "es2015": "dist/esm/index.js",
    "es2017": "dist/esm/index.js",
    "jsnext:main": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/components/components.js",
    "files": [
        "dist/"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/",
        "access": "restricted"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:EventStore/Design-System.git",
        "directory": "packages/components"
    },
    "scripts": {
        "build": "stencil build --docs",
        "start": "stencil build --dev --watch --serve",
        "docs": "run-s \"docs:*\"",
        "docs:stencil": "stencil build --docs-json ../../documentation/generated/components.stencil.json",
        "docs:typedoc": "typedoc --logLevel Error --json ../../documentation/generated/components.typedoc.json",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "lint": "yarn g:lint"
    },
    "dependencies": {
        "@floating-ui/dom": "0.4.1"
    },
    "peerDependencies": {
        "@eventstore/router": "0.1.x",
        "@eventstore/theme": "0.1.x",
        "@eventstore/utils": "0.1.x"
    },
    "devDependencies": {
        "@eventstore/configs": "workspace:*",
        "@eventstore/router": "workspace:*",
        "@eventstore/theme": "workspace:*",
        "@eventstore/utils": "workspace:*",
        "@stencil/core": "^2.15.0",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "puppeteer": "^10.2.0",
        "typedoc": "^0.22.13",
        "typescript": "4.5.5"
    },
    "stableVersion": "0.0.66"
}
