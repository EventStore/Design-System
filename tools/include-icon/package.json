{
    "name": "@eventstore/include-icon",
    "version": "0.0.59",
    "description": "Automated icon inclusion for Event Store design system",
    "license": "Apache-2.0",
    "bin": {
        "icon": "./dist/index.js"
    },
    "files": [
        "dist/"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/",
        "access": "restricted"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:EventStore/Design-System.git",
        "directory": "tools/include-icon"
    },
    "scripts": {
        "build": "tsc --outDir ./dist --declaration",
        "dev": "tsc --outDir ./dist --declaration --watch",
        "test": "jest --passWithNoTests",
        "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier --check \"src/**/**/!(*.d).{ts,tsx,css,json,html}\""
    },
    "prettier": "@eventstore/configs/prettier",
    "dependencies": {
        "camelcase": "^6.0.0",
        "clipboardy": "^2.3.0",
        "prettier": "^2.2.1",
        "svgo": "^1.3.2",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "@types/svgo": "^1.3.3",
        "typescript": "^4.1.3"
    }
}
